#!/bin/sh

set -e

. /usr/share/debconf/confmodule

db_get snf-image/dlurl
URL="$RET"

if [ -n "$URL" ]; then
    snf-image-update-helper -y -u "$URL"
else
    echo "You can later download the snf-image-helper image using:"
    echo
    echo "    snf-image-update-helper"
    echo
fi


#DEBHELPER#

exit 0

